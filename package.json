{
  "name": "@userscripters/generate-headers",
  "version": "1.1.0",
  "description": "Userscript manager header generator",
  "icon": "",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "lint": "eslint src --fix --ext .js,.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run prebuild && tsc --outDir dist && npm run executable",
    "executable": "sed -i \"1s/^/#!\\/usr\\/bin\\/env node\\n/\" dist/index.js",
    "publish:patch": "npm run build && npm version patch && git push",
    "publish:minor": "npm run build && npm version minor && git push",
    "publish:major": "npm run build && npm version major && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/userscripters/template.git"
  },
  "keywords": [
    "userscripts",
    "tampermonkey",
    "greasemonkey",
    "violentmonkey",
    "stackoverflow"
  ],
  "author": "Oleg Valter",
  "contributors": [],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/userscripters/template/issues"
  },
  "homepage": "https://github.com/userscripters/template#readme",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "@types/tampermonkey": "^4.0.0",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "mocha": "^8.4.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "uglify-js": "^3.13.8"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "yargs": "^16.2.0"
  },
  "main": "dist/index.js",
  "bin": {
    "generate": "dist/index.js"
  }
}

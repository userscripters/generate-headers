{
  "name": "@userscripters/generate-headers",
  "version": "1.6.4",
  "description": "Userscript manager header generator",
  "icon": "",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "lint": "eslint src --fix --ext .js,.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run test && npm run lint && npm run clean",
    "build": "tsc --outDir dist && npm run executable",
    "executable": "sed -i \"1s/^/#!\\/usr\\/bin\\/env node\\n/\" dist/index.js",
    "version:patch": "npm run build && npm version patch",
    "version:minor": "npm run build && npm version minor",
    "version:major": "npm run build && npm version major",
    "postversion": "generate-readme && git add . && git commit --amend --no-edit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/userscripters/generate-headers.git"
  },
  "keywords": [
    "userscripts",
    "tampermonkey",
    "greasemonkey",
    "violentmonkey",
    "stackoverflow"
  ],
  "author": {
    "name": "Oleg Valter",
    "email": "oleg.a.valter@gmail.com"
  },
  "contributors": [
    {
      "name": "double beep",
      "url": "https://github.com/double-beep"
    }
  ],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/userscripters/generate-headers/issues"
  },
  "homepage": "https://github.com/userscripters/generate-headers#readme",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.4.10",
    "@types/tampermonkey": "^4.0.2",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@userscripters/generate-readme": "^1.1.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "mocha": "^9.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "uglify-js": "^3.14.1"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "yargs": "^17.0.1"
  },
  "main": "dist/index.js",
  "bin": {
    "generate": "dist/index.js"
  }
}
